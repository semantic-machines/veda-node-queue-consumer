    "eslint-config-google": "^0.14.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security-node": "^1.1.0",
    "nyc": "^15.1.0"
  },
  "dependencies": {
    "loglevel": "^1.8.0",
    "nanomsg": "^4.1.0"
  }
