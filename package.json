{
  "name": "n-vqueue",
  "version": "1.0.0",
  "description": "Veda queue consumer for Node.js",
  "main": "index.node",
  "repository": {
    "type": "git",
    "url": "git://github.com/semantic-machines/veda-node-queue-consumer.git"
  },
  "scripts": {
